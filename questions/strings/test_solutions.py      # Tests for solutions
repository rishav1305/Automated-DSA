class TestStringSolutions(unittest.TestCase):

    def test_easy_solutions(self):
        # Example test for an easy string solution
        from solution_file_name import function_name  # Replace with actual solution file and function
        self.assertEqual(function_name("input"), "expected_output")  # Replace with actual input and expected output

    def test_medium_solutions(self):
        # Example test for a medium string solution
        from solution_file_name import function_name  # Replace with actual solution file and function
        self.assertEqual(function_name("input"), "expected_output")  # Replace with actual input and expected output

if __name__ == '__main__':
    unittest.main()